}
#3
#include <stdio.h>
int main(void)
{
	int age,days;
	printf("注意，这是一个转换天数的工具\n你只需输入你的年龄，系统会自动计算出你活了多少天\n");
	scanf("%d",&age);
	printf("恭喜你,你已经活了%d天！继续加油哦",age*365);
	getchar();
	getchar();
	return 0; 
}
#4
#include <stdio.h>
void jolly(void);
int main(void)
{
	jolly();
	jolly();
	jolly();
	printf("Which nobody can deny!");
	getchar();
	return 0;
}
void jolly(void)
{
	printf("For he's a jolly good fellow!\n");
	
}
